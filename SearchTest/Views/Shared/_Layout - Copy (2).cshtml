<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Tate Art Gallery Explorer - Azure Search Demo</title>
    <!-- Bootstrap Core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="css/blog-home.css" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Navigation -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Tate Art Gallery Explorer - Demo</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>
    <!-- Page Content -->
    <div class="container">

        <div class="row">
            <!-- Blog Sidebar Widgets Column -->
            <div class="col-md-4">
                <!-- Blog Search Well -->
                <div class="well">
                    <h4>Gallery Search</h4>
                    <div class="input-group">
                        <input type="text" placeholder="Search for art (such as computer)..." class="form-control" id="q" oninput="execSearch();">
                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button">
                                <span class="glyphicon glyphicon-search"></span>
                            </button>
                        </span>
                    </div>
                    <!-- /.input-group -->
                </div>
                <!-- Blog Categories Well -->
                <div class="well">
                    <h4>Top Related Subjects</h4>
                    <p id="currentSubject"></p>
                    <div class="row">
                        <div class="col-lg-12">
                            <ul class="list-unstyled" id="subjectsContainer"></ul>
                        </div>
                        <!-- /.col-lg-6 -->
                    </div>
                    <!-- /.row -->
                </div>
                <!-- Blog Categories Well -->
                <div class="well">
                    <h4>Top Related Contributors</h4>
                    <p id="currentContributor"></p>
                    <div class="row">
                        <div class="col-lg-12">
                            <ul class="list-unstyled" id="contributorsContainer"></ul>
                        </div>
                        <!-- /.col-lg-6 -->
                    </div>
                    <!-- /.row -->
                </div>
            </div>


            <!-- Blog Entries Column -->

            <div class="col-md-8">
                <nav>
                    <ul class="pagination" id="pagination">
                        <li class=""><a href="javascript:void(0);" onclick="ChangePage(this);" aria-label="Previous" id="PagingPrevious"><span aria-hidden="true">&laquo;</span></a></li>
                        <li class="active"><a href="javascript:void(0);" onclick="ChangePage(this);" id="Paging1">1</a></li>
                        <li class=""><a href="javascript:void(0);" onclick="ChangePage(this);" id="Paging2">2</span></a></li>
                        <li class=""><a href="javascript:void(0);" onclick="ChangePage(this);" id="Paging3">3</span></a></li>
                        <li class=""><a href="javascript:void(0);" onclick="ChangePage(this);" id="Paging4">4</span></a></li>
                        <li class=""><a href="javascript:void(0);" onclick="ChangePage(this);" id="Paging5">5</span></a></li>
                        <li class=""><a href="javascript:void(0);" onclick="ChangePage(this);" aria-label="Next" id="PagingNext"><span aria-hidden="true">&raquo;</span></a></li>
                    </ul>
                </nav>
            </div>

            <div class="col-md-8">
                <div id="mediaContainer">
                </div>
            </div>
            @RenderBody()
        </div>
        <!-- /.row -->
        <hr>
        <!-- Footer -->
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p>Content provided by the <a href="https://github.com/tategallery/collection">Tate Art Gallery</a> via <a href="https://azure.microsoft.com/en-us/services/search/">Azure Search</a></p>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </footer>
    </div>
    <!-- /.container -->
    <!-- jQuery -->
    <script src="js/jquery.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>
    <!-- Azure Search Code -->
    <script src="js/search.js"></script>
</body>
</html>



